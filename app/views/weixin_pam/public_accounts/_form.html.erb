<%= simple_form_for(@public_account) do |f| %>
  <%= f.input :name  %>
  <%= f.input :app_id  %>
  <%= f.input :app_secret  %>
  <%= f.input :api_url  %>
  <%= f.input :api_token  %>
  <%= f.input :host  %>
  <%= f.input :reply_class  %>
  <%= f.input :enabled  %>
  <%= f.submit '保存', class: 'btn btn-primary' %>
  <%= link_to '返回', @public_account.new_record? ? public_accounts_path : @public_account, class: 'btn btn-default'  %>
<% end %>
